    {
      "version": 2,
      "builds": [
        {
          "src": "api/classifyWaste.py",
          "use": "@vercel/python",
          "config": {
            "maxLambdaSize": "50mb"
          }
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/api/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/frontend/$1"
        }
      ]
    }
    